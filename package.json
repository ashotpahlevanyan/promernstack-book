    "start-hook": "nodemon -w server server/start_hook.js",
    "dev": "npm run watch & npm run look & npm run watch-server & npm start",
    "dev-all-hook": "npm run watch & npm run look & npm run start-hook",
    "lint": "eslint --ext js,jsx src server webpack.config.js",
    "lint-watch": "esw --ext js,jsx src server webpack.config.js -w"
